{
  "name": "jos-e2e",
  "version": "2.0.0",
  "description": "jOS e2e Test Scenarios",
  "scripts": {
    "check-format": "prettier --check .",
    "delete-reports": "rimraf ./reports/* || true",
    "pretest": "npm run delete-reports",
    "postinstall": "npx playwright install",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts --fix --max-warnings=0",
    "check-all": "npm run format && npm run check-lint",
    "test": "playwright test",
    "report": "npx playwright show-report ./reports/html",
    "prepare": "husky install"
  },
  "keywords": [
    "jOS",
    "Playwright",
    "e2e"
  ],
  "authors": [
    "Dzmitry_Shaplyka@epam.com",
    "Antanina_Kostuseva@epam.com"
  ],
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "1.38.0",
    "@reportportal/agent-js-playwright": "^5.1.3",
    "@types/html-to-text": "^9.0.2",
    "@types/lodash": "^4.14.198",
    "@types/node": "20.6.2",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "html-to-text": "^9.0.5",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "winston": "^3.10.0",
    "xml2js": "^0.6.2"
  }
}
